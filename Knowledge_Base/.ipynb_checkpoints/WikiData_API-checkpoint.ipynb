    "from urllib2 import Request, urlopen, URLError\n",
    "\n",
    "request = Request('https://en.wikipedia.org/w/api.php')\n",
    "\n",
    "try:\n",
    "    response = urlopen(request)\n",
    "    kittens = response.read()\n",
    "    print kittens[559:1000]\n",
    "except URLError, e:\n",
    "    print 'Got an error code:', e"
